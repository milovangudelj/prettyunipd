{
	"short_name": "React App",
	"name": "Pretty Unipd",
	"version": "1.0.0",
	"manifest_version": 3,
	"description": "Make Unipd.it great again",
	"icons": {
		"256": "/favicon.png"
	},
	"permissions": [],
	"content_scripts": [
		{
			"matches": ["https://*.unipd.it/*", "http://*.unipd.it/*"],
			"js": ["scripts/content.js"]
		}
	],
	"host_permissions": ["<all_urls>"],
	"action": {
		"default_title": "Pretty Unipd",
		"default_popup": "index.html",
		"default_icon": {
			"256": "/favicon.png"
		}
	}
}